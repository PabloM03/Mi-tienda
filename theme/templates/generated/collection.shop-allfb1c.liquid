<script>
document.addEventListener('DOMContentLoaded', function() {
    function calculateDeliveryDate() {
        const today = new Date();

        function addBusinessDays(date, days) {
            let count = 0;
            let tempDate = new Date(date);
            while (count < days) {
                tempDate.setDate(tempDate.getDate() + 1);
                if (tempDate.getDay() !== 0 && tempDate.getDay() !== 6) {
                    count++;
                }
            }
            return tempDate;
        }

        function formatDate(date) {
            const dayName = date.toLocaleString('en-US', { weekday: 'short' }); // Obtiene el nombre abreviado del día
            const day = date.getDate();
            // Obtener el nombre del mes con las primeras tres letras
            const month = date.toLocaleString('default', { month: 'short' });
            
            // Formatear el nombre del mes: primera letra en mayúscula y sin punto al final
            const formattedMonth = month.charAt(0).toUpperCase() + month.slice(1, 3);
             // Obtiene el nombre abreviado del mes
            const year = date.getFullYear();
            return `${dayName}, ${day} ${formattedMonth} ${year}`;
        }

        const deliveryStartDate = addBusinessDays(today, 9);
        const deliveryEndDate = addBusinessDays(today, 12);

        return {
            start: formatDate(deliveryStartDate),
            end: formatDate(deliveryEndDate)
        };
    }

    function updateShippingMessage() {
        const deliveryDateRange = calculateDeliveryDate();
        const messageContainer = document.getElementById('shipping-message');
        messageContainer.textContent = `Estimated Delivery:  ${deliveryDateRange.start} - ${deliveryDateRange.end}`;
    }

    function checkCartVisibility() {
        const cartDrawer = document.getElementById('cart-drawer');
        if (cartDrawer.style.display !== 'none') {
            updateShippingMessage();
        }
    }

    // Verificar la visibilidad del carrito cada 500ms
    setInterval(checkCartVisibility, 500);

    // También podemos adjuntar el evento al botón del carrito
    document.querySelector('.icon-cart').addEventListener('click', updateShippingMessage);
});

    </script>

